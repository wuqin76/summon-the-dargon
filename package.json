{
  "name": "dragon-spin-game",
  "version": "1.0.0",
  "description": "Telegram Web App - Dragon Spin Game with TRC20 USDT",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node scripts/add-ip-columns.js && node dist/server.js",
    "start:simple": "node dist/server.js",
    "start:simple": "node dist/server.js",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:railway": "node scripts/migrate-railway.js",
    "migrate:add-ip": "node scripts/add-ip-columns.js",
    "seed": "ts-node src/database/seed.ts",
    "test": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "verify-config": "node scripts/verify-config.js",
    "check": "node scripts/pre-deploy-check.js",
    "pre-deploy": "node scripts/pre-deploy-check.js"
  },
  "keywords": [
    "telegram",
    "webapp",
    "trc20",
    "usdt",
    "spin",
    "game"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-telegram-bot-api": "^0.64.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "tronweb": "^5.3.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.19.25"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
